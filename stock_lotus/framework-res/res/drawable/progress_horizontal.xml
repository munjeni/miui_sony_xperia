<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@id/background">
        <shape>
            <corners android:radius="2.0px" />
            <gradient android:startColor="#1a000000" android:endColor="#26ffffff" android:angle="270.0" />
            <stroke android:width="1.0px" android:color="#cc7f7f7f" />
        </shape>
    </item>
    <item android:id="@id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="2.0px" />
                <gradient android:startColor="#7f2aabe4" android:endColor="#4c2aabe4" android:angle="270.0" />
            </shape>
        </clip>
    </item>
    <item android:id="@id/progress">
        <clip>
            <shape>
                <corners android:radius="2.0px" />
                <gradient android:startColor="#ff2aabe4" android:endColor="#99267598" android:angle="270.0" />
            </shape>
        </clip>
    </item>
</layer-list>
