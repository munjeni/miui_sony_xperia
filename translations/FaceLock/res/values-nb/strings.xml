    <string name="tutorial_app_name">Om Ansiktslås</string>
    <string name="tutorial_header">Se på telefonen for å låse den opp.</string>
    <string name="tutorial_info_text">"Husk på følgende:

"<ul><li>"Ansiktslås er mindre sikkert enn opplåsingsmønster, PIN-kode eller passord.

"<li>Noen som ligner på deg kan klare å låse opp telefonen din.</li>"

"<li>Dataene som brukes for å identifisere ansiktet ditt lagres privat på telefonen.</li>"

"</li></ul></string>
    <string name="tutorial_continue_label">Start konfigurasjon</string>
    <string name="intro_header">Slik oppnår du best resultat:</string>
    <string name="intro_info_text">" "<ul><li>"Finn et sted innendørs som ikke har for sterk eller svak belysning.

"<li>Hold telefonen på nivå med øynene dine.</li>"

"</li></ul></string>
    <string name="intro_continue_label">Fortsett</string>
    <string name="endscreen_header">Det var det!</string>
    <string name="endscreen_info_text">"Lås opp telefonen ved å se på den.

Tips: Forbedre ansiktsgjenkjenningen ved å ta bilde av ansiktet ditt under ulike forhold."</string>
    <string name="endscreen_continue_label">OK</string>
